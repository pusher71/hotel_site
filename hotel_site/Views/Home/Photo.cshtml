@model IEnumerable<IPhoto>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Photo</title>
</head>
<body>
    <div class="grid-photo-container">
        @foreach (IPhoto photo in Model)
        {
            <img class="grid-photo-item" src="data:image/jpeg;base64,@Convert.ToBase64String(photo.GetImage())" />
        }
    </div>
</body>
</html>
